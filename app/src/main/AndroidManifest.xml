        android:supportsRtl="true"
        android:theme="@style/Theme.ProjetoMovimenta">

        <meta-data android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyCZdZRQsgv1JadBXwzPT--R35OwMjBCViY" />


        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".FormCadastro"
            android:exported="false" />

        <!-- Outros atributos da atividade, se houver -->

        <activity
            android:name=".FormLogin"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <!-- Outras atividades (Activities) do seu aplicativo -->

    </application>
</manifest>
